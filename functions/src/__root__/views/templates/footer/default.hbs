<footer id="templateFooter">

  <div class="container">

    <div class="country-region-selects">

      <label for="language">{{translate 'LANGUAGE'}}:</label>

      <select
        class="country-region-selects-select"
        data-select-target="language"
        name="Language"
        >

        {{#if settings.languages}}

          {{#eachProperty settings.languages}}

            <option
              value="{{key}}"
              {{#is key ../settings.language}}
                selected="selected"
              {{/is}}
              >

              {{value}}

            </option>

          {{/eachProperty}}

        {{/if}}

      </select>

    </div>

    <div class="footer-links">

      <a href="" data-target="open-contact-modal">{{translate 'CONTACT-US'}}</a> |

      <a href="/{{settings.region}}/{{settings.language}}/terms/">{{translate 'TERMS'}}</a> |

      <a href="/{{settings.region}}/{{settings.language}}/privacy/">{{translate 'PRIVACY'}}</a>

    </div>

  </div>

</footer>
